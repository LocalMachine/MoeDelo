<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestSuite" parallel="tests" thread-count="4">
    <listeners>
        <listener class-name="Settings.ScreenshotListener"/>
    </listeners>
    <test name="Test for Chrome browser">
        <parameter name="browser" value="chrome"/>
        <parameter name="version" value="92.0"/>
        <classes>
            <class name="Tests.SignInTest"/>
        </classes>
    </test>
    <test name="Test for Firefox browser">
        <parameter name="browser" value="firefox"/>
        <parameter name="version" value="91.0"/>
        <classes>
            <class name="Tests.SignInTest"/>
        </classes>
    </test>
</suite>